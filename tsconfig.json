/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
    "compileOnSave": false,
    "compilerOptions": {
        "outDir": "./dist/out-tsc",
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noImplicitOverride": true,
        "noPropertyAccessFromIndexSignature": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "skipLibCheck": true,
        "paths": {
            "@geometricpanda/ng-storyblok": ["./projects/geometricpanda/ng-storyblok/src/index.ts"],
            "@geometricpanda/ng-storyblok/config": [
                "./projects/geometricpanda/ng-storyblok/config/index.ts"
            ],
            "@geometricpanda/ng-storyblok/render": [
                "./projects/geometricpanda/ng-storyblok/render/index.ts"
            ],
            "@geometricpanda/ng-storyblok/router": [
                "./projects/geometricpanda/ng-storyblok/router/index.ts"
            ],
            "@geometricpanda/ng-storyblok/services": [
                "./projects/geometricpanda/ng-storyblok/services/index.ts"
            ],
            "@geometricpanda/ng-storyblok/tokens": [
                "./projects/geometricpanda/ng-storyblok/tokens/index.ts"
            ],
            "@geometricpanda/ng-storyblok/pipes": [
                "./projects/geometricpanda/ng-storyblok/pipes/index.ts"
            ]
        },
        "esModuleInterop": true,
        "sourceMap": true,
        "declaration": false,
        "experimentalDecorators": true,
        "moduleResolution": "node",
        "importHelpers": true,
        "target": "ES2022",
        "module": "ES2022",
        "useDefineForClassFields": false,
        "lib": ["ES2022", "dom", "dom.iterable"]
    },
    "angularCompilerOptions": {
        "enableI18nLegacyMessageIdFormat": false,
        "strictInjectionParameters": true,
        "strictInputAccessModifiers": true,
        "strictTemplates": true
    }
}
